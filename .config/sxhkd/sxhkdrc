#
# wm independent hotkeys
#
#
# media keys
#
super + F7
#	mpc next
	playerctl next
super + F6
# mpc toggle
	playerctl play-pause
super + F5
#	mpc prev
	playerctl previous
super + F8
#	mpc stop
	playerctl stop
super + F9
#	mpc seekthrough -5
	playerctl position 5-
super + F10
#	mpc seekthrough +5
	playerctl position 5+
super + shift + e
	shutdmenu
super + c
	clipmenu
super + shift + a
	dmenuumount
super + a
	dmenumount
super + shift + u
	dmenuunicode
super + w
	firefox
super + shift + c
	dunstctl context
